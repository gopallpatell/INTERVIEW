- name: Install Git and Maven on all servers
  hosts: all
  become: yes
  tasks:
    - name: Install Git on Debian/Ubuntu
      apt:
        name: git
        state: present
      when: ansible_os_family == "Debian"

    - name: Install Maven on Debian/Ubuntu
      apt:
        name: maven
        state: present
      when: ansible_os_family == "Debian"

